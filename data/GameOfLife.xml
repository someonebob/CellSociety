<?xml version="1.0" encoding="UTF-8" standalone="no"?><CA>
	<subclass>simulation.LifeRules</subclass>

	<title>Conways's Game of Life</title>
	<author>Dhruv Patel</author>
	
	<cyclesPerTick>1</cyclesPerTick>

	<stateDef color="black" ref="o">alive</stateDef>
	<stateDef color="white" ref="x">dead</stateDef>
	
	<scheme>
		<default>
			<alive>black</alive>
			<dead>white</dead>
		</default>

		<negative>
			<alive>white</alive>
			<dead>black</dead>
		</negative>

		<terminal>
			<alive>lime</alive>
			<dead>black</dead>
		</terminal>
		
		<colorblind>
			<alive>lightsalmon</alive>
			<dead>coral</dead>
		</colorblind>
		
	</scheme>
	
	<state>
	x x x x x
	x x o x x
	x x o x x
	x x o x x 
	x x x x x	
	</state>
</CA>